<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <originTimeRange>
                <startTime>
                    <year>2003</year>
                    <month>1</month>
                    <day>1</day>
                </startTime>
                <endTime>
                    <year>2003</year>
                    <month>1</month>
                    <day>10</day>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>5</min>
            </magnitudeRange>
            <catalog>WHDF</catalog>
        </eventFinder>
        <originOR>
            <originAND>
                <magnitudeRange>
                    <min>5</min>
                </magnitudeRange>
                <originDepthRange>
                    <unit>KILOMETER</unit>
                    <min>100</min>
                    <max>200</max>
                </originDepthRange>
            </originAND>
            <magnitudeRange>
                <min>6</min>
            </magnitudeRange>
        </originOR>
        <jythonOriginSubsetter>
            <module>originExample</module>
            <class>OriginExample</class>
        </jythonOriginSubsetter>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
        </networkFinder>
        <networkAND>
	    <networkCode>II</networkCode>
            <jythonNetworkSubsetter>
                <module>networkExample</module>
                <class>NetworkExample</class>
            </jythonNetworkSubsetter>
	</networkAND>
        <jythonStationSubsetter>
            <module>stationExample</module>
            <class>StationExample</class>
	    <stationCode>HOPE</stationCode>
        </jythonStationSubsetter>
        <jythonSiteSubsetter>
            <module>siteExample</module>
            <class>SiteExample</class>
        </jythonSiteSubsetter>
        <jythonChannelSubsetter>
            <module>channelExample</module>
            <class>ChannelCodeIsBHZ</class>
        </jythonChannelSubsetter>
        <bandCode>B</bandCode>
        <printlineChannelProcess/>
    </networkArm>
    <waveformArm>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>ttp</beginPhase>
            <beginOffset>
                <unit>SECOND</unit>
                <value>-120</value>
            </beginOffset>
            <endPhase>tts</endPhase>
            <endOffset>
                <unit>SECOND</unit>
                <value>360</value>
            </endOffset>
        </phaseRequest>
        <fixedDataCenter>
            <name>IRIS_PondDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <availableDataAND>
            <someCoverage/>
        <jythonAvailableDataSubsetter>
            <module>availableDataExample</module>
            <class>AvailableSod</class>
        </jythonAvailableDataSubsetter>
        </availableDataAND>
        <printlineSeismogramProcess/>
        <jythonWaveformProcess>
            <module>waveformExample</module>
            <class>HelloSod</class>
        </jythonWaveformProcess>
    </waveformArm>
</sod>
