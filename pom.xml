  <scm>
    <url>http://roo.seis.sc.edu/fisheye/viewrep/seis/trunk/${pom.artifactId}</url>
  </scm>
  <organization>
    <name>Seismology at the University of South Carolina</name>
    <url>http://www.seis.sc.edu</url>
  </organization>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <testSourceDirectory>test/unit</testSourceDirectory>
    <resources>
      <resource>
        <targetPath>edu/sc/seis/sod/editor</targetPath>
        <directory>src/edu/sc/seis/sod/editor</directory>
        <includes>
          <include>names.prop</include>
          <include>*.gif</include>
          <include>*.png</include>
        </includes>
      </resource>
      <resource>
        <targetPath>edu/sc/seis/sod/database/waveform</targetPath>
        <directory>src/edu/sc/seis/sod/database/waveform</directory>
        <includes>
          <include>*.vm</include>
        </includes>
      </resource>
      <resource>
        <targetPath>edu/sc/seis/sod/database/props</targetPath>
        <directory>src/edu/sc/seis/sod/database/props</directory>
        <includes>
          <include>*.props</include>
          <include>*.vm</include>
        </includes>
      </resource>
      <resource>
        <targetPath>edu/sc/seis/sod/data/configFiles</targetPath>
        <directory>scripts</directory>
        <includes>
          <include>weed.xml</include>
          <include>demo.xml</include>
          <include>standingOrder.xml</include>
          <include>breqfast.xml</include>
          <include>vector.xml</include>
        </includes>
      </resource>
      <resource>
        <targetPath>edu/sc/seis/sod/tools</targetPath>
        <directory>src/edu/sc/seis/sod/tools</directory>
        <includes>
          <include>*.props</include>
          <include>*.vm</include>
        </includes>
      </resource>
      <resource>
        <targetPath>edu/sc/seis/sod/data</targetPath>
        <directory>src/edu/sc/seis/sod/data</directory>
        <includes>
          <include>**/*</include>
        </includes>
      </resource>
      <resource>
        <targetPath>edu/sc/seis/sod/hibernate</targetPath>
        <directory>src/edu/sc/seis/sod/hibernate</directory>
        <includes>
          <include>**/*.xml</include>
        </includes>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <targetPath>edu/sc/seis/sod/data</targetPath>
        <directory>test/unit/edu/sc/seis/sod/data</directory>
        <includes>
          <include>**/*</include>
        </includes>
      </testResource>
    </testResources>
    <plugins>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <configuration>
          <doctitle>SOD</doctitle>
          <links>http://www.seis.sc.edu/software/fissuresIDL/apidocs,http://www.seis.sc.edu/software/fissuresImpl/apidocs,http://www.seis.sc.edu/software/SeedCodec/apidocs</links>
          <windowtitle>SOD</windowtitle>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <excludes>
            <exclude>XMLConfigUtil.java</exclude>
            <exclude>**/CoverageTestData.java</exclude>
          </excludes>
          <includes>
            <include>**/*Test*.java</include>
          </includes>
        </configuration>
      </plugin>
    </plugins>
  </build>
