  <version>3.0.0alpha</version>
  <description>SOD automates the most tedious data selection, downloading and routine processing tasks, shortening the
        time between idea and science. Please also check out the user site at http://www.seis.sc.edu/sod.</description>
  <url>http://www.seis.sc.edu/software/${pom.artifactId}</url>
  <ciManagement>
    <notifiers>
      <notifier>
        <configuration>
          <address>jaster@seis.sc.edu</address>
        </configuration>
      </notifier>
    </notifiers>
  </ciManagement>
  <inceptionYear>2002</inceptionYear>
  <mailingLists>
    <mailingList>
      <name>sod</name>
      <subscribe>http://mail.seis.sc.edu/mailman/listinfo/sod</subscribe>
      <unsubscribe>http://mail.seis.sc.edu/mailman/listinfo/sod</unsubscribe>
    </mailingList>
  </mailingLists>
  <developers>
    <developer>
      <id>crotwell</id>
      <name>Philip Crotwell</name>
      <organization>University of South Carolina</organization>
      <roles>
        <role>Module Maintainer</role>
        <role>Java Developer</role>
