<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <originTimeRange>
                <startTime>
                    <earlier>
                        <timeInterval>
                            <value>4</value>
                            <unit>WEEK</unit>
                        </timeInterval>
                    </earlier>
                </startTime>
                <endTime>
                    <later>
                        <timeInterval>
                            <value>50</value>
                            <unit>GREGORIAN_YEAR</unit>
                        </timeInterval>
                    </later>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>6</min>
            </magnitudeRange>
            <catalog>PREF</catalog>
        </eventFinder>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
        </networkFinder>
        <stationArea>
            <boxArea>
                <latitudeRange>
                    <min>42</min>
                    <max>49</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-124</min>
                    <max>-112</max>
                </longitudeRange>
            </boxArea>
        </stationArea>
        <channelOR>
            <!-- accept broadband-->
            <channelAND>
                <bandCode>B</bandCode>
                <gainCode>H</gainCode>
            </channelAND>
            <channelAND>
                <!-- accept short period only if not broadband-->
                <bandCode>S</bandCode>
                <gainCode>H</gainCode>
                <channelNOT>
                    <stationHas>
                        <channelAND>
                            <bandCode>B</bandCode>
                            <gainCode>H</gainCode>
                        </channelAND>
                    </stationHas>
                </channelNOT>
            </channelAND>
            <channelAND>
                <!-- accept extreme short period only if no broadband and no short period-->
                <bandCode>E</bandCode>
                <gainCode>H</gainCode>
                <channelNOT>
                    <stationHas>
                        <channelOR>
                            <channelAND>
                                <bandCode>B</bandCode>
                                <gainCode>H</gainCode>
                            </channelAND>
                            <channelAND>
                                <bandCode>S</bandCode>
                                <gainCode>H</gainCode>
                            </channelAND>
                        </channelOR>
                    </stationHas>
                </channelNOT>
            </channelAND>
            <channelNOT>
                <!-- if no BH?, SH? or EH?, then take all channels-->
                <stationHas>
                    <channelOR>
                        <channelAND>
                            <bandCode>B</bandCode>
                            <gainCode>H</gainCode>
                        </channelAND>
                        <channelAND>
                            <bandCode>S</bandCode>
                            <gainCode>H</gainCode>
                        </channelAND>
                        <channelAND>
                            <bandCode>E</bandCode>
                            <gainCode>H</gainCode>
                        </channelAND>
                    </channelOR>
                </stationHas>
            </channelNOT>
        </channelOR>
        <printlineChannelProcessor/>
    </networkArm>
    <waveformArm>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>origin</beginPhase>
            <beginOffset>
                <unit>SECOND</unit>
                <value>0</value>
            </beginOffset>
            <endPhase>origin</endPhase>
            <endOffset>
                <unit>HOUR</unit>
                <value>2</value>
            </endOffset>
        </phaseRequest>
        <fixedDataCenter>
            <name>IRIS_BudDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <someCoverage/>
        <printlineSeismogramProcess/>
        <saveSeismogramToFile/>
    </waveformArm>
</sod>
