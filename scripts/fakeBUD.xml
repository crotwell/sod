<?xml version="1.0"?>
<sod xmlns:xlink="http://www.w3.org/1999/xlink/">
    <properties>
        <runName>Fake BUD</runName>
        <waveformWorkerThreads>2</waveformWorkerThreads>
        <eventChannelPairProcessing>atLeastOnce</eventChannelPairProcessing>
        <checkpointPeriodically>TRUE</checkpointPeriodically>
    </properties>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <boxArea>
                <latitudeRange>
                    <min>-90</min>
                    <max>90</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-180</min>
                    <max>180</max>
                </longitudeRange>
            </boxArea>
            <originDepthRange>
                <unit>KILOMETER</unit>
                <min>0</min>
                <max>1000</max>
            </originDepthRange>
            <originTimeRange>
                <startTime>
                    <earlier>
                        <timeInterval>
                            <value>30</value>
                            <unit>DAY</unit>
                        </timeInterval>
                    </earlier>
                </startTime>
                <endTime>21050912T17:53:00.001Z</endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>3.5</min>
                <max>10.0</max>
            </magnitudeRange>
            <catalog>FINGER</catalog>
        </eventFinder>
        <originAND>
            <removeEventDuplicate/>
            <originOR>
                <youngerThan>
                    <unit>WEEK</unit>
                    <value>2</value>
                </youngerThan>
                <originAND>
                    <magnitudeRange>
                        <min>7</min>
                    </magnitudeRange>
                    <youngerThan>
                        <unit>GREGORIAN_YEAR</unit>
                        <value>1</value>
                    </youngerThan>
                </originAND>
                <originAND>
                    <magnitudeRange>
                        <min>8</min>
                    </magnitudeRange>
                    <youngerThan>
                        <value>4</value>
                        <unit>GREGORIAN_YEAR</unit>
                    </youngerThan>
                </originAND>
                <magnitudeRange>
                    <min>9</min>
                </magnitudeRange>
            </originOR>
            <originOR>
                <magnitudeRange>
                    <min>5.5</min>
                </magnitudeRange>
                <originOR>
                    <originPointDistance>
                        <latitude>47</latitude>
                        <longitude>-95</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>47</latitude>
                        <longitude>-124</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>42</latitude>
                        <longitude>-124</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>32</latitude>
                        <longitude>-124</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>20</latitude>
                        <longitude>-95</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>25</latitude>
                        <longitude>-74</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>35</latitude>
                        <longitude>-75</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                    <originPointDistance>
                        <latitude>47</latitude>
                        <longitude>-65</longitude>
                        <unit>DEGREE</unit>
                        <max>30</max>
                    </originPointDistance>
                </originOR>
            </originOR>
        </originAND>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
            <refreshInterval>
                <unit>HOUR</unit>
                <value>50</value>
            </refreshInterval>
        </networkFinder>
        <networkNOT>
            <temporaryNetwork/>
        </networkNOT>
        <stationAND>
            <stationEffectiveTimeOverlap>
                <startTime>
                    <earlier>
                        <timeInterval>
                            <value>1</value>
                            <unit>GREGORIAN_YEAR</unit>
                        </timeInterval>
                    </earlier>
                </startTime>
                <endTime>21050912T17:53:00.001Z</endTime>
            </stationEffectiveTimeOverlap>
        </stationAND>
        <siteAND>
            <siteOR>
                <siteCode/>
                <siteCode>00</siteCode>
            </siteOR>
            <siteEffectiveTimeOverlap>
                <startTime>
                    <earlier>
                        <timeInterval>
                            <value>1</value>
                            <unit>GREGORIAN_YEAR</unit>
                        </timeInterval>
                    </earlier>
                </startTime>
                <endTime>21050912T17:53:00.001Z</endTime>
            </siteEffectiveTimeOverlap>
        </siteAND>
        <channelAND>
            <channelOR>
                <bandCode>B</bandCode>
                <bandCode>S</bandCode>
            </channelOR>
            <gainCode>H</gainCode>
            <channelEffectiveTimeOverlap>
                <startTime>
                    <earlier>
                        <timeInterval>
                            <value>1</value>
                            <unit>GREGORIAN_YEAR</unit>
                        </timeInterval>
                    </earlier>
                </startTime>
                <endTime>21050912T17:53:00.001Z</endTime>
            </channelEffectiveTimeOverlap>
            <hadDataLastWeek>
                <fixedDataCenter>
                    <name>IRIS_BudDataCenter</name>
                    <dns>edu/iris/dmc</dns>
                </fixedDataCenter>
            </hadDataLastWeek>
        </channelAND>
    </networkArm>
    <waveformVectorArm>
        <eventStationAND>
            <embeddedOriginSubsetter>
                <originOR>
                    <youngerThan>
                        <unit>WEEK</unit>
                        <value>2</value>
                    </youngerThan>
                    <originAND>
                        <magnitudeRange>
                            <min>7</min>
                        </magnitudeRange>
                        <youngerThan>
                            <unit>GREGORIAN_YEAR</unit>
                            <value>1</value>
                        </youngerThan>
                    </originAND>
                    <originAND>
                        <magnitudeRange>
                            <min>8</min>
                        </magnitudeRange>
                        <youngerThan>
                            <value>4</value>
                            <unit>GREGORIAN_YEAR</unit>
                        </youngerThan>
                    </originAND>
                    <magnitudeRange>
                        <min>9</min>
                    </magnitudeRange>
                </originOR>
            </embeddedOriginSubsetter>
            <eventStationOR>
                <eventStationAND>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>180</max>
                    </distanceRange>
                    <embeddedOriginSubsetter>
                        <magnitudeRange>
                            <min>6.5</min>
                        </magnitudeRange>
                    </embeddedOriginSubsetter>
                </eventStationAND>
                <eventStationAND>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>5</max>
                    </distanceRange>
                    <embeddedOriginSubsetter>
                        <magnitudeRange>
                            <min>3.0</min>
                            <lessThan>4</lessThan>
                        </magnitudeRange>
                    </embeddedOriginSubsetter>
                </eventStationAND>
                <eventStationAND>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>10</max>
                    </distanceRange>
                    <embeddedOriginSubsetter>
                        <magnitudeRange>
                            <min>4.0</min>
                            <lessThan>5.2</lessThan>
                        </magnitudeRange>
                    </embeddedOriginSubsetter>
                </eventStationAND>
                <eventStationAND>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>20</max>
                    </distanceRange>
                    <embeddedOriginSubsetter>
                        <magnitudeRange>
                            <min>5.2</min>
                            <lessThan>5.7</lessThan>
                        </magnitudeRange>
                    </embeddedOriginSubsetter>
                </eventStationAND>
                <eventStationAND>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>30</max>
                    </distanceRange>
                    <embeddedOriginSubsetter>
                        <magnitudeRange>
                            <min>5.7</min>
                            <lessThan>6.0</lessThan>
                        </magnitudeRange>
                    </embeddedOriginSubsetter>
                </eventStationAND>
                <eventStationAND>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>90</max>
                    </distanceRange>
                    <embeddedOriginSubsetter>
                        <magnitudeRange>
                            <min>6.0</min>
                            <lessThan>6.5</lessThan>
                        </magnitudeRange>
                    </embeddedOriginSubsetter>
                </eventStationAND>
            </eventStationOR>
        </eventStationAND>
        <choiceRequest>
            <choice>
                <embeddedEventStation>
                    <eventStationOR>
                        <eventStationAND>
                            <distanceRange>
                                <unit>DEGREE</unit>
                                <min>0</min>
                                <max>5</max>
                            </distanceRange>
                            <embeddedOriginSubsetter>
                                <magnitudeRange>
                                    <min>3.0</min>
                                    <lessThan>4</lessThan>
                                </magnitudeRange>
                            </embeddedOriginSubsetter>
                        </eventStationAND>
                        <eventStationAND>
                            <distanceRange>
                                <unit>DEGREE</unit>
                                <min>0</min>
                                <max>10</max>
                            </distanceRange>
                            <embeddedOriginSubsetter>
                                <magnitudeRange>
                                    <min>4.0</min>
                                    <lessThan>5.2</lessThan>
                                </magnitudeRange>
                            </embeddedOriginSubsetter>
                        </eventStationAND>
                    </eventStationOR>
                </embeddedEventStation>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>SECOND</unit>
                        <value>0</value>
                    </beginOffset>
                    <endPhase>origin</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>10</value>
                    </endOffset>
                </phaseRequest>
            </choice>
            <choice>
                <embeddedEventStation>
                    <eventStationOR>
                        <eventStationAND>
                            <distanceRange>
                                <unit>DEGREE</unit>
                                <min>0</min>
                                <max>20</max>
                            </distanceRange>
                            <embeddedOriginSubsetter>
                                <magnitudeRange>
                                    <min>5.2</min>
                                    <lessThan>5.7</lessThan>
                                </magnitudeRange>
                            </embeddedOriginSubsetter>
                        </eventStationAND>
                        <eventStationAND>
                            <distanceRange>
                                <unit>DEGREE</unit>
                                <min>0</min>
                                <max>30</max>
                            </distanceRange>
                            <embeddedOriginSubsetter>
                                <magnitudeRange>
                                    <min>5.7</min>
                                    <lessThan>6.0</lessThan>
                                </magnitudeRange>
                            </embeddedOriginSubsetter>
                        </eventStationAND>
                    </eventStationOR>
                </embeddedEventStation>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>SECOND</unit>
                        <value>0</value>
                    </beginOffset>
                    <endPhase>origin</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>25</value>
                    </endOffset>
                </phaseRequest>
            </choice>
            <choice>
                <embeddedEventStation>
                    <eventStationAND>
                        <distanceRange>
                            <unit>DEGREE</unit>
                            <min>0</min>
                            <max>90</max>
                        </distanceRange>
                        <embeddedOriginSubsetter>
                            <magnitudeRange>
                                <min>6.0</min>
                                <lessThan>6.5</lessThan>
                            </magnitudeRange>
                        </embeddedOriginSubsetter>
                    </eventStationAND>
                </embeddedEventStation>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>SECOND</unit>
                        <value>0</value>
                    </beginOffset>
                    <endPhase>origin</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>35</value>
                    </endOffset>
                </phaseRequest>
            </choice>
            <otherwise>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>SECOND</unit>
                        <value>0</value>
                    </beginOffset>
                    <endPhase>origin</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>90</value>
                    </endOffset>
                </phaseRequest>
            </otherwise>
        </choiceRequest>
        <choiceDataCenter>
            <choice>
                <embeddedEventStation>
                    <embeddedOriginSubsetter>
                        <originTimeRange>
                            <startTime>
                                <earlier>
                                    <timeInterval>
                                        <value>1</value>
                                        <unit>GREGORIAN_YEAR</unit>
                                    </timeInterval>
                                </earlier>
                            </startTime>
                            <endTime>
                                <earlier>
                                    <timeInterval>
                                        <value>2</value>
                                        <unit>WEEK</unit>
                                    </timeInterval>
                                </earlier>
                            </endTime>
                        </originTimeRange>
                    </embeddedOriginSubsetter>
                </embeddedEventStation>
                <fixedDataCenter>
                    <name>IRIS_PondDataCenter</name>
                    <dns>edu/iris/dmc</dns>
                </fixedDataCenter>
            </choice>
            <otherwise>
                <fixedDataCenter>
                    <name>IRIS_BudDataCenter</name>
                    <dns>edu/iris/dmc</dns>
                </fixedDataCenter>
            </otherwise>
        </choiceDataCenter>
        <printlineSeismogramProcess/>
        <saveSeismogramToFile>
            <fileType>mseed</fileType>
            <dataDirectory>FakeBUD</dataDirectory>
            <eventDirLabel>Event_$event.getTime("yyyy_DDD_HH_mm_ss")</eventDirLabel>
            <storeSeismogramsInDB/>
        </saveSeismogramToFile>
    </waveformVectorArm>
</sod>
