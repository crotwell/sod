<?xml version="1.0"?>
<sod>
    <properties>
        <seismogramLatency>
            <unit>DAY</unit>
            <value>5</value>
        </seismogramLatency>
    </properties>
    <eventArm>
        <fdsnEvent>
            <originTimeRange>
                <startTime>
                    <earlier>
                        <unit>DAY</unit>
                        <value>1</value>
                    </earlier>
                </startTime>
                <endTime>
                  <later>
                    <unit>DAY</unit>
                    <value>50</value>
                  </later>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>3</min>
            </magnitudeRange>
            <catalog>NEIC PDE</catalog>
        </fdsnEvent>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <fdsnStation>
        </fdsnStation>
        <networkCode>II</networkCode>
        <bandCode>B</bandCode>
        <printlineChannelProcess/>
    </networkArm>
    <waveformArm>
        <distanceRange>
            <unit>DEGREE</unit>
            <min>30</min>
            <max>90</max>
        </distanceRange>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>ttp</beginPhase>
            <beginOffset>
                <unit>SECOND</unit>
                <value>-120</value>
            </beginOffset>
            <endPhase>tts</endPhase>
            <endOffset>
                <unit>SECOND</unit>
                <value>360</value>
            </endOffset>
        </phaseRequest>
        <fdsnDataSelect/>
        <availableDataAND>
            <availableDataOR>
                <fullCoverage/>
                <postEventWait>
                    <unit>DAY</unit>
                    <value>4</value>
                </postEventWait>
            </availableDataOR>
            <someCoverage/>
        </availableDataAND>
        <printlineSeismogramProcess/>
        <sacWriter/>
    </waveformArm>
</sod>
