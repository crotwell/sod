<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <boxArea>
                <latitudeRange>
                    <min>-90</min>
                    <max>90</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-180</min>
                    <max>-1180</max>
                </longitudeRange>
            </boxArea>
            <originTimeRange>
                <startTime>20030101T00:00:00.001Z</startTime>
                <endTime>20040131T23:59:59.999Z</endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>5.0</min>
            </magnitudeRange>
            <contributor>NEIC</contributor>
        </eventFinder>
        <magnitudeRange>
            <magType>mb</magType>
            <magType>M</magType>
            <min>5.0</min>
        </magnitudeRange>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
        </networkFinder>
        <networkOR>
            <networkCode>IU</networkCode>
            <networkCode>IC</networkCode>
            <networkCode>II</networkCode>
        </networkOR>
        <channelOR>
            <bandCode>B</bandCode>
            <bandCode>L</bandCode>
        </channelOR>
        <responseWriter/>
    </networkArm>
    <waveformVectorArm>
        <distanceRange>
            <unit>DEGREE</unit>
            <max>100</max>
        </distanceRange>
        <eventChannelOR>
            <eventChannelAND>
                <embeddedChannel>
                    <bandCode>B</bandCode>
                </embeddedChannel>
                <embeddedEventStation>
                    <distanceRange>
                        <unit>KILOMETER</unit>
                        <max>1000</max>
                    </distanceRange>
                </embeddedEventStation>
            </eventChannelAND>
            <eventChannelAND>
                <embeddedChannel>
                    <bandCode>L</bandCode>
                </embeddedChannel>
                <embeddedEventStation>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <max>100</max>
                    </distanceRange>
                </embeddedEventStation>
            </eventChannelAND>
        </eventChannelOR>
        <choiceRequest>
            <choice>
                <embeddedEventStation>
                    <distanceRange>
                        <unit>KILOMETER</unit>
                        <max>1000</max>
                    </distanceRange>
                </embeddedEventStation>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>MINUTE</unit>
                        <value>-3</value>
                    </beginOffset>
                    <endPhase>1.5kmps</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>3</value>
                    </endOffset>
                </phaseRequest>
            </choice>
            <choice>
                <embeddedChannel>
                    <bandCode>B</bandCode>
                </embeddedChannel>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>ttp</beginPhase>
                    <beginOffset>
                        <unit>MINUTE</unit>
                        <value>-3</value>
                    </beginOffset>
                    <endPhase>tts</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>6</value>
                    </endOffset>
                </phaseRequest>
            </choice>
            <otherwise>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>ttp</beginPhase>
                    <beginOffset>
                        <unit>MINUTE</unit>
                        <value>-3</value>
                    </beginOffset>
                    <endPhase>2kmps</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>6</value>
                    </endOffset>
                </phaseRequest>
            </otherwise>
        </choiceRequest>
        <fixedDataCenter>
            <name>IRIS_PondDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <someCoverage/>
        <printlineSeismogramProcess/>
        <saveSeismogramToFile>
            <fileType>sac</fileType>
            <dataDirectory>rawSeismograms</dataDirectory>
        </saveSeismogramToFile>
        <rMean/>
        <rotateGCP/>
        <saveSeismogramToFile>
            <fileType>sac</fileType>
            <dataDirectory>processedSeismograms</dataDirectory>
        </saveSeismogramToFile>
    </waveformVectorArm>
</sod>
