<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <boxArea>
                <latitudeRange>
                    <min>46</min>
                    <max>49</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-124</min>
                    <max>-116</max>
                </longitudeRange>
            </boxArea>
            <originTimeRange>
                <startTime>
                    <earlier>
                        <timeInterval>
                            <value>4</value>
                            <unit>WEEK</unit>
                        </timeInterval>
                    </earlier>
                </startTime>
                <endTime>
                    <later>
                        <timeInterval>
                            <value>50</value>
                            <unit>GREGORIAN_YEAR</unit>
                        </timeInterval>
                    </later>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>3</min>
            </magnitudeRange>
            <catalog>PREF</catalog>
        </eventFinder>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
        </networkFinder>
        <stationOR>
            <stationPointDistance>
                <!-- Minnesota -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>47</latitude>
                <longitude>-95</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- Washington -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>47</latitude>
                <longitude>-124</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- N. Calif -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>42</latitude>
                <longitude>-124</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- S. Calif -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>32</latitude>
                <longitude>-124</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- Texas -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>20</latitude>
                <longitude>-95</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- Florida -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>25</latitude>
                <longitude>-74</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- Virgina -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>35</latitude>
                <longitude>-75</longitude>
            </stationPointDistance>
            <stationPointDistance>
                <!-- Maine -->
                <unit>DEGREE</unit>
                <max>30</max>
                <latitude>47</latitude>
                <longitude>-65</longitude>
            </stationPointDistance>
        </stationOR>
        <bandCode>B</bandCode>
        <printlineChannelProcessor/>
    </networkArm>
    <waveformArm>
        <choiceRequest>
            <choice>
                <embeddedEventStation>
                    <distanceRange>
                        <unit>DEGREE</unit>
                        <max>15</max>
                    </distanceRange>
                </embeddedEventStation>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>SECOND</unit>
                        <value>0</value>
                    </beginOffset>
                    <endPhase>origin</endPhase>
                    <endOffset>
                        <unit>SECOND</unit>
                        <value>360</value>
                    </endOffset>
                </phaseRequest>
            </choice>
            <otherwise>
                <phaseRequest>
                    <model>prem</model>
                    <beginPhase>origin</beginPhase>
                    <beginOffset>
                        <unit>MINUTE</unit>
                        <value>0</value>
                    </beginOffset>
                    <endPhase>origin</endPhase>
                    <endOffset>
                        <unit>MINUTE</unit>
                        <value>30</value>
                    </endOffset>
                </phaseRequest>
            </otherwise>
        </choiceRequest>
        <fixedDataCenter>
            <name>IRIS_BUDDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <someCoverage/>
        <printlineSeismogramProcess/>
        <saveSeismogramToFile>
            <fileType>mseed</fileType>
            <dataDirectory>seismograms</dataDirectory>
            <eventDirLabel>Event_$event.getTime("yyyy_DDD_HH_mm_ss")</eventDirLabel>
            <storeSeismogramsInDB/>
        </saveSeismogramToFile>
    </waveformArm>
</sod>
