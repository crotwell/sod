<channelScript type="jython">
rot=75
if channel.orientationCode == "1" or channel.orientationCode == "N":
    channel.getWrapped().orientation.azimuth = rot
    channel.getWrapped().orientation.dip = 0
if channel.orientationCode == "2" or channel.orientationCode == "E":
    channel.getWrapped().orientation.azimuth = 90+rot
    channel.getWrapped().orientation.dip = 0
