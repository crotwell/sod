<?xml version="1.0"?>
<sod>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <originTimeRange>
                <startTime>
                    <year>2005</year>
                    <month>8</month>
                    <day>2</day>
                </startTime>
                <endTime>
                    <year>2005</year>
                    <month>9</month>
                    <day>15</day>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>6</min>
            </magnitudeRange>
            <catalog>WHDF</catalog>
        </eventFinder>
       <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>SNEPNetwork</name>
            <dns>edu/sc/seis/test</dns>
        </networkFinder>
        <bandCode>B</bandCode>
        <printlineChannelProcess/>
    </networkArm>
    <waveformArm>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>ttp</beginPhase>
            <beginOffset>
                <unit>MINUTE</unit>
                <value>-20</value>
            </beginOffset>
            <endPhase>tts</endPhase>
            <endOffset>
                <unit>MINUTE</unit>
                <value>60</value>
            </endOffset>
        </phaseRequest>
        <fixedDataCenter>
            <name>SNEPSeismogram</name>
            <dns>edu/sc/seis/test</dns>
        </fixedDataCenter>
        <someCoverage/>
        <printlineSeismogramProcess/>
        <mseedWriter/>
        <seismogramImageProcess/>
    </waveformArm>
</sod>
