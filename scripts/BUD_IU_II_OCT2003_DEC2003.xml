<?xml version="1.0"?>
<sod xmlns:xlink="http://www.w3.org/1999/xlink/">
        <properties>
                <property>
                        <name>edu.sc.seis.sod.databasetype</name>
                        <value>edu.sc.seis.sod.database.HSqlDbManager</value>
                </property>
                <property>
                        <name>edu.sc.seis.sod.daystoincrement</name>
                        <value>1</value>
                </property>
                <property>
                        <name>edu.sc.seis.sod.database.getNewEvents</name>
                        <value>true</value>
                </property>
                <property>
                        <name>edu.sc.seis.sod.database.eventRefreshInterval</name>
                        <value>21600</value>
                </property>
                <property>
                        <name>edu.sc.seis.sod.database.reopenEvents</name>
                        <value>true</value>
                </property>
        </properties>
        <eventArm>
                <eventFinder>
                        <name>IRIS_EventDC</name>
                        <dns>edu/iris/dmc</dns>
                        <boxArea>
                                <latitudeRange>
                                        <min>-90</min>
                                        <max>90</max>
                                </latitudeRange>
                                <longitudeRange>
                                        <min>-180</min>
                                        <max>180</max>
                                </longitudeRange>
                        </boxArea>
                        <originDepthRange>
                                        <unit>KILOMETER</unit>
                                        <min>0</min>
                                        <max>1000</max>
                        </originDepthRange>
                        <eventTimeRange>
                                <timeRange>
                                        <startTime>20031010T00:00:00.001Z</startTime>
                                        <endTime>20031231T23:59:59.999Z</endTime>
                                </timeRange>
                        </eventTimeRange>
                        <magnitudeRange>
                                <magType>%</magType>
                                <min>2.0</min>
                                <max>10.0</max>
                        </magnitudeRange>
                        <catalog>FINGER</catalog>
                        <contributor>NEIC</contributor>
                </eventFinder>
                <mapEventStatus xlink:href="status/eventMap.png"/>
                <eventStatusTemplate
                        outputLocation="status/time.html" xlink:href="jar:edu/sc/seis/sod/data/templates/eventArm/timeSorted.xml"/>
                <eventStatusTemplate
                        outputLocation="status/depth.html" xlink:href="jar:edu/sc/seis/sod/data/templates/eventArm/depthSorted.xml"/>
                <eventStatusTemplate
                        outputLocation="status/location.html" xlink:href="jar:edu/sc/seis/sod/data/templates/eventArm/locSorted.xml"/>
                <eventStatusTemplate
                        outputLocation="status/mag.html" xlink:href="jar:edu/sc/seis/sod/data/templates/eventArm/magSorted.xml"/>

        </eventArm>
        <networkArm>
                <networkFinder>
                        <name>IRIS_NetworkDC</name>
                        <dns>edu/iris/dmc</dns>
                        <refreshInterval>
                                <unit>MINUTE</unit>
                                <value>3000</value>
                        </refreshInterval>
                </networkFinder>
                <networkOR>
                        <networkCode>II</networkCode>
                        <networkCode>IU</networkCode>
                </networkOR>
                <stationAND>
                        <stationOR>
                                <stationCode>ADK</stationCode>
                                <stationCode>KIP</stationCode>
                                <stationCode>TEIG</stationCode>
                                <stationCode>PET</stationCode>
                                <stationCode>SBA</stationCode>
                                <stationCode>LVZ</stationCode>
                                <stationCode>RPN</stationCode>
                                <stationCode>EFI</stationCode>
                                <stationCode>KIV</stationCode>
                                <stationCode>KDAK</stationCode>
                        </stationOR>
                        <stationEffectiveTimeOverlap>
                                <effectiveTimeOverlap>
                                        <min>20031010T00:00:00.001Z</min>
                                        <max>20031231T23:59:59.999Z</max>
                                </effectiveTimeOverlap>
                        </stationEffectiveTimeOverlap>
                </stationAND>
                <siteEffectiveTimeOverlap>
                        <effectiveTimeOverlap>
                                <min>20031010T00:00:00.001Z</min>
                                <max>20031231T23:59:59.999Z</max>
                        </effectiveTimeOverlap>
                </siteEffectiveTimeOverlap>
                <channelAND>
                        <bandCode>B</bandCode>
                        <channelNOT>
                                <!-- to prevent LOG channels -->
                                <gainCode>O</gainCode>
                        </channelNOT>
                        <channelEffectiveTimeOverlap>
                                <effectiveTimeOverlap>
                                        <min>20031010T00:00:00.001Z</min>
                                        <max>20031231T23:59:59.999Z</max>
                                </effectiveTimeOverlap>
                        </channelEffectiveTimeOverlap>
                </channelAND>
                <networkStatusTemplate
                    outputLocation="status/network.html" xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/network.xml" />
                <networkInfoTemplateGenerator>
                    <fileLoc>status/<code/>-<firstWord/>/stations.html</fileLoc>
                    <externalConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/stations.xml"/>
                </networkInfoTemplateGenerator>
        </networkArm>
        <waveFormArm>
                <distanceRange>
                        <unit>DEGREE</unit>
                        <min>0</min>
                        <max>180</max>
                </distanceRange>
                <localSeismogramArm>
                        <phaseRequest>
                                <model>prem</model>
                                <beginPhase>ttp</beginPhase>
                                <beginOffset>
                                        <unit>SECOND</unit>
                                        <value>-60</value>
                                </beginOffset>
                                <endPhase>tts</endPhase>
                                <endOffset>
                                        <unit>SECOND</unit>
                                        <value>360</value>
                                </endOffset>
                        </phaseRequest>
                        <fixedDataCenter>
                                <name>IRIS_BudDataCenter</name>
                                <dns>edu/iris/dmc</dns>
                        </fixedDataCenter>
                        <someCoverage/>
                        <printlineSeismogramProcess/>
                        <sacFileProcessor>
                                <dataDirectory>BUD</dataDirectory>
                                <eventDirLabel> Event_<originTime>yyyy_DDD_HH_mm_ss</originTime>
                                </eventDirLabel>
                        </sacFileProcessor>
                </localSeismogramArm>
                <waveformEventTemplateGenerator>
                        <fileLoc>status/<originTime>yyyyMMddHHmmssSSS</originTime>/event.html</fileLoc>
                        <externalConfig xlink:href="jar:edu/sc/seis/sod/data/templates/waveformArm/waveformEvent.xml"/>
                </waveformEventTemplateGenerator>
        </waveFormArm>
</sod>
