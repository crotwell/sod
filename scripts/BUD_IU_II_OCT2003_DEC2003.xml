<?xml version="1.0"?>
<sod xmlns="http://www.seis.sc.edu/xschema/sod/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.seis.sc.edu/xschema/sod/1.0 http://www.seis.sc.edu/xschema/sod/1.0/sod.xsd">
<!--this xml config is to test SOD.  It gets data from 10/10/03 until
     12/31/03 from ten pretty much random stations in the IU and II 
     networks for any events with a magnitude of 2 or higher in the 
     FINGER catalog.  It gets B channels for these stations and saves
     them in the BUD directory                                  -->
  <properties>
    <property>
      <name>edu.sc.seis.sod.databasetype</name>
      <value>edu.sc.seis.sod.database.HSqlDbManager</value>
    </property>
<!-- number of days to get events per request to eventFinder -->
    <property>
      <name>edu.sc.seis.sod.daystoincrement</name>
      <value>1</value>
    </property>
<!-- don't go back and reask server for events that may have
         appeared in server after this was first run. -->
    <property>
      <name>edu.sc.seis.sod.database.getNewEvents</name>
      <value>true</value>
    </property>
<!-- how frequently sod updates its list of events, units are seconds 
     this translates to four times a day -->
    <property>
      <name>edu.sc.seis.sod.database.eventRefreshInterval</name>
      <value>21600</value>
    </property>
    <property>
      <name>edu.sc.seis.sod.database.reopenEvents</name>
      <value>true</value>
    </property>
  </properties>
  <eventArm>
    <LogFactor5Status>
      <classname>edu.sc.seis.sod.subbsetter.eventArm.LogFactor5Status</classname>
    </LogFactor5Status>
    <eventFinder>
      <name>IRIS_EventDC</name>
      <dns>edu/iris/dmc</dns>
      <boxArea>
        <latitudeRange>
          <min>-90</min>
          <max>90</max>
        </latitudeRange>
        <longitudeRange>
          <min>-180</min>
          <max>180</max>
        </longitudeRange>
      </boxArea>
      <originDepthRange>
        <unitRange>
          <unit>KILOMETER</unit>
          <min>0</min>
          <max>1000</max>
        </unitRange>
      </originDepthRange>
      <eventTimeRange>
        <timeRange>
          <startTime>20031010T00:00:00.001Z</startTime>
          <endTime>20031231T23:59:59.999Z</endTime>
        </timeRange>
      </eventTimeRange>
      <magnitudeRange>
        <magType>%</magType>
        <min>2.0</min>
        <max>10.0</max>
      </magnitudeRange>
      <catalog>
        <value>FINGER</value>
      </catalog>
      <contributor>NEIC</contributor>
    </eventFinder>
  </eventArm>
  <networkArm>
    <LogFactor5Status>
      <classname>edu.sc.seis.sod.subbsetter.networkArm.LogFactor5Status</classname>
    </LogFactor5Status>
    <SimpleHTMLStatus>
      <classname>edu.sc.seis.sod.subbsetter.networkArm.SimpleHTMLStatus</classname>
    </SimpleHTMLStatus>
    <networkFinder>
      <description>Connect to the IRIS networkDC</description>
      <name>IRIS_NetworkDC</name>
      <dns>edu/iris/dmc</dns>
      <refreshInterval>
        <unit>MINUTE</unit>
        <value>3000</value>
      </refreshInterval>
    </networkFinder>
    <networkOR>
      <networkCode>II</networkCode>
      <networkCode>IU</networkCode>
    </networkOR>
    <stationAND>
      <stationOR>
        <stationCode>ADK</stationCode>
        <stationCode>KIP</stationCode>
        <stationCode>TEIG</stationCode>
        <stationCode>PET</stationCode>
        <stationCode>SBA</stationCode>
        <stationCode>LVZ</stationCode>
        <stationCode>RPN</stationCode>
        <stationCode>EFI</stationCode>
        <stationCode>KIV</stationCode>
        <stationCode>KDAK</stationCode>
      </stationOR>
      <stationEffectiveTimeOverlap>
        <effectiveTimeOverlap>
          <min>20031010T00:00:00.001Z</min>
          <max>20031231T23:59:59.999Z</max>
        </effectiveTimeOverlap>
      </stationEffectiveTimeOverlap>
    </stationAND>
    <siteEffectiveTimeOverlap>
      <effectiveTimeOverlap>
        <min>20031010T00:00:00.001Z</min>
        <max>20031231T23:59:59.999Z</max>
      </effectiveTimeOverlap>
    </siteEffectiveTimeOverlap>
    <channelAND>
      <bandCode>B</bandCode>
      <channelNOT>
<!-- to prevent LOG channels -->
        <gainCode>O</gainCode>
      </channelNOT>
      <channelEffectiveTimeOverlap>
        <effectiveTimeOverlap>
          <min>20031010T00:00:00.001Z</min>
          <max>20031231T23:59:59.999Z</max>
        </effectiveTimeOverlap>
      </channelEffectiveTimeOverlap>
    </channelAND>
    <printlineChannelProcessor/>
  </networkArm>
  <waveFormArm>
    <distanceRange>
      <unit>DEGREE</unit>
      <min>0</min>
      <max>180</max>
    </distanceRange>
    <localSeismogramArm>
      <fixedDataCenter>
        <name>IRIS_BudDataCenter</name>
        <dns>edu/iris/dmc</dns>
      </fixedDataCenter>
      <phaseRequest>
        <model>prem</model>
        <beginPhase>ttp</beginPhase>
        <beginOffset>
          <unit>SECOND</unit>
          <value>-60</value>
        </beginOffset>
        <endPhase>tts</endPhase>
        <endOffset>
          <unit>SECOND</unit>
          <value>360</value>
        </endOffset>
      </phaseRequest>
      <someCoverage/>
      <printlineSeismogramProcess/>
      <sacFileProcessor>
        <dataDirectory>BUD</dataDirectory>
        <eventDirLabel>
        Event_
        <originTime>yyyy_DDD_HH_mm_ss</originTime>
      </eventDirLabel>
      </sacFileProcessor>
    </localSeismogramArm>
  </waveFormArm>
</sod>
