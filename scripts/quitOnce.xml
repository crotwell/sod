<sod>
    <properties>
        <eventLag>
            <unit>HOUR</unit>
            <value>0</value>
        </eventLag>
    </properties>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <originDepthRange>
                <unit>KILOMETER</unit>
                <min>0</min>
                <max>10000</max>
            </originDepthRange>
            <originTimeRange>
                <startTime>
                    2007-01-13T00:00:00.0000Z
                </startTime>
                <endTime>
                    <now/>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>8</min>
                <max>10</max>
            </magnitudeRange>
            <catalog>PREF</catalog>
        </eventFinder>
        <printlineEventProcess>
            <template>$event.getLongitude('##0.0000;-##0.0000') $event.getLatitude('##0.0000;-##0.0000') $event.getDepth('###0.##') ${event.getTime('yyyy_DDD_HH_mm_sss')} $event.magnitudeValue$event.magnitudeType</template>
        </printlineEventProcess>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
        </networkFinder>
        <networkOR>
            <networkCode>IU</networkCode>
        </networkOR>
        <stationAND>
            <stationOR>
                <stationCode>ANMO</stationCode>
            </stationOR>
            <printlineStationProcess>
                <template>$station.getLongitude(' ##0.0000;-##0.0000') $station.getLatitude(' ##0.0000;-##0.0000') $station.getElevation('###0.') $station.code</template>
            </printlineStationProcess>
        </stationAND>
        <channelCode>BHZ</channelCode>
    </networkArm>
    <waveformArm>
        <phaseRequest>
            <beginPhase>origin</beginPhase>
            <beginOffset>
                <unit>MINUTE</unit>
                <value>-10</value>
            </beginOffset>
            <endPhase>ttp</endPhase>
            <endOffset>
                <unit>MINUTE</unit>
                <value>20</value>
            </endOffset>
        </phaseRequest>
        <fixedDataCenter>
            <name>IRIS_BudDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <externalWaveformProcess>
            <classname>edu.sc.seis.sod.process.waveform.QuitOnce</classname>
        </externalWaveformProcess>
        <sacWriter/>
    </waveformArm>
</sod>
