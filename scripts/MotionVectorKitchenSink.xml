<?xml version="1.0" encoding="UTF-8"?>
<sod xmlns:xlink="http://www.w3.org/1999/xlink/">
	<properties>
		<runName>MotionVector KitchenSink</runName>
		<statusBase>status</statusBase>
		<eventQueryIncrement>
			<value>1</value>
			<unit>WEEK</unit>
		</eventQueryIncrement>
		<eventRefreshInterval>
			<value>6</value>
			<unit>MINUTE</unit>
		</eventRefreshInterval>
		<eventLag>
			<value>1</value>
			<unit>WEEK</unit>
		</eventLag>
		<maxRetryDelay>
			<value>16</value>
			<unit>DAY</unit>
		</maxRetryDelay>
		<waveformWorkerThreads>1</waveformWorkerThreads>
		<removeDatabase>TRUE</removeDatabase>
	</properties>
	<eventArm>
		<eventFinder>
			<name>IRIS_EventDC</name>
			<dns>edu/iris/dmc</dns>
			<boxArea>
				<latitudeRange>
					<unit>DEGREE</unit>
					<min>-90</min>
					<max>90</max>
				</latitudeRange>
				<longitudeRange>
					<unit>DEGREE</unit>
					<min>-180</min>
					<max>180</max>
				</longitudeRange>
			</boxArea>
			<originDepthRange>
					<min>0</min>
					<max>1000</max>
			</originDepthRange>
			<originTimeRange>
				<startTime>20030101T00:00:00.001Z</startTime>
				<endTime>20030131T23:59:59.999Z</endTime>
			</originTimeRange>
			<magnitudeRange>
				<min>6.0</min>
				<max>10.0</max>
			</magnitudeRange>
			<catalog>PREF</catalog>
			<contributor>IRIS</contributor>
		</eventFinder>
		<removeEventDuplicate/>
		<printLineEventProcess/>
		<eventStatusTemplate>
			<eventConfig xlink:href="jar:edu/sc/seis/sod/data/templates/eventArm/eventStatus.xml"/>
		</eventStatusTemplate>
	</eventArm>
	<networkArm>
		<networkFinder>
			<name>IRIS_NetworkDC</name>
			<dns>edu/iris/dmc</dns>
			<refreshInterval>
				<unit>HOUR</unit>
				<value>50</value>
			</refreshInterval>
		</networkFinder>
		<networkCode>II</networkCode>
		<stationEffectiveTimeOverlap>
			<startTime>20030101T00:00:00.001Z</startTime>
			<endTime>20030131T23:59:59.999Z</endTime>
		</stationEffectiveTimeOverlap>
		<siteAND>
			<siteOR>
				<siteCode></siteCode>
				<siteCode>00</siteCode>
			</siteOR>
			<siteEffectiveTimeOverlap>
				<startTime>20030101T00:00:00.001Z</startTime>
				<endTime>20030131T23:59:59.999Z</endTime>
			</siteEffectiveTimeOverlap>
		</siteAND>
		<channelAND>
			<bandCode>L</bandCode>
			<channelNOT>
                                <!-- to prevent LOG channels -->
				<gainCode>O</gainCode>
			</channelNOT>
			<channelEffectiveTimeOverlap>
				<startTime>20030101T00:00:00.001Z</startTime>
				<endTime>20030131T23:59:59.999Z</endTime>
			</channelEffectiveTimeOverlap>
		</channelAND>
		<networkInfoTemplateGenerator>
			<netConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/networks.xml"/>
			<stationConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/stations.xml"/>
			<channelConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/channels.xml"/>
		</networkInfoTemplateGenerator>
	</networkArm>
	<waveformArm>
		<motionVectorArm>
			<eventChannelGroupAND>
				<eventChannelGroupOR>
					<ANDEventChannelWrapper>
						<embeddedChannel>
							<bandCode>B</bandCode>
						</embeddedChannel>
					</ANDEventChannelWrapper>
					<passEventChannel/>
				</eventChannelGroupOR>
				<eventChannelGroupOR>
					<OREventChannelWrapper>
						<embeddedChannel>
							<bandCode>B</bandCode>
						</embeddedChannel>
					</OREventChannelWrapper>
					<passEventChannel/>
				</eventChannelGroupOR>
				<eventChannelGroupOR>
					<eventChannelGroupXOR>
						<ANDEventChannelWrapper>
							<embeddedChannel>
								<bandCode>B</bandCode>
							</embeddedChannel>
						</ANDEventChannelWrapper>
						<ANDEventChannelWrapper>
							<embeddedChannel>
								<bandCode>B</bandCode>
							</embeddedChannel>
						</ANDEventChannelWrapper>
					</eventChannelGroupXOR>
					<passEventChannel/>
				</eventChannelGroupOR>
				<eventChannelGroupOR>
					<eventChannelGroupNOT>
						<OREventChannelWrapper>
							<embeddedChannel>
								<bandCode>B</bandCode>
							</embeddedChannel>
						</OREventChannelWrapper>
					</eventChannelGroupNOT>
					<passEventChannel/>
				</eventChannelGroupOR>
			</eventChannelGroupAND>
			<requestGeneratorWrapper>
				<phaseRequest>
					<model>prem</model>
					<beginPhase>ttp</beginPhase>
					<beginOffset>
						<unit>SECOND</unit>
						<value>-120</value>
					</beginOffset>
					<endPhase>2.0kmps</endPhase>
					<endOffset>
						<unit>SECOND</unit>
						<value>60</value>
					</endOffset>
				</phaseRequest>
			</requestGeneratorWrapper>
			<channelGroupRequestSubsetterAND>
				<channelGroupRequestSubsetterOR>
					<ANDRequestSubsetterWrapper>
						<breqFastRequestSubsetter>
							<dataDirectory>request</dataDirectory>
							<name>Bob the Seismologist</name>
							<inst>USC</inst>
							<mail>200 Main Street, GreatCity,USA</mail>
							<email>bob@seis.sc.edu</email>
							<phone>8033332223</phone>
							<fax>none</fax>
							<media>Electronic</media>
							<altmedia1>Electronic</altmedia1>
							<altmedia2>Electronic</altmedia2>
							<quality>A</quality>
						</breqFastRequestSubsetter>
					</ANDRequestSubsetterWrapper>
					<passChannelGroupRequest/>
				</channelGroupRequestSubsetterOR>
				<channelGroupRequestSubsetterOR>
					<ORRequestSubsetterWrapper>
						<breqFastRequestSubsetter>
							<dataDirectory>request</dataDirectory>
							<name>Bob the Seismologist</name>
							<inst>USC</inst>
							<mail>200 Main Street, GreatCity,USA</mail>
							<email>bob@seis.sc.edu</email>
							<phone>8033332223</phone>
							<fax>none</fax>
							<media>Electronic</media>
							<altmedia1>Electronic</altmedia1>
							<altmedia2>Electronic</altmedia2>
							<quality>A</quality>
						</breqFastRequestSubsetter>
					</ORRequestSubsetterWrapper>
					<passChannelGroupRequest/>
				</channelGroupRequestSubsetterOR>
				<channelGroupRequestSubsetterOR>
					<channelGroupRequestSubsetterXOR>
						<ANDRequestSubsetterWrapper>
                            <requestPrint/>
                        </ANDRequestSubsetterWrapper>
						<ANDRequestSubsetterWrapper>
                            <requestPrint/>
                        </ANDRequestSubsetterWrapper>
					</channelGroupRequestSubsetterXOR>
					<passChannelGroupRequest/>
				</channelGroupRequestSubsetterOR>
				<channelGroupRequestSubsetterOR>
					<channelGroupRequestSubsetterNOT>
						<ANDRequestSubsetterWrapper>
                            <requestPrint/>
                        </ANDRequestSubsetterWrapper>
					</channelGroupRequestSubsetterNOT>
					<passChannelGroupRequest/>
				</channelGroupRequestSubsetterOR>
			</channelGroupRequestSubsetterAND>
			<fixedDataCenter>
				<name>IRIS_BudDataCenter</name>
				<dns>edu/iris/dmc</dns>
			</fixedDataCenter>
			<channelGroupAvailableDataAND>
				<channelGroupAvailableDataOR>
					<ANDAvailableDataWrapper>
						<noGaps/>
					</ANDAvailableDataWrapper>
					<passChannelGroupAvailableData/>
				</channelGroupAvailableDataOR>
				<channelGroupAvailableDataOR>
					<ORAvailableDataWrapper>
						<someCoverage/>
					</ORAvailableDataWrapper>
					<passChannelGroupAvailableData/>
				</channelGroupAvailableDataOR>
				<channelGroupAvailableDataOR>
					<channelGroupAvailableDataXOR>
						<ANDAvailableDataWrapper>
                            <noGaps/>
                        </ANDAvailableDataWrapper>
						<ANDAvailableDataWrapper>
                            <noGaps/>
                        </ANDAvailableDataWrapper>
					</channelGroupAvailableDataXOR>
					<passChannelGroupAvailableData/>
				</channelGroupAvailableDataOR>
				<channelGroupAvailableDataOR>
					<channelGroupAvailableDataNOT>
						<ANDAvailableDataWrapper>
                            <noGaps/>
                        </ANDAvailableDataWrapper>
					</channelGroupAvailableDataNOT>
					<passChannelGroupAvailableData/>
				</channelGroupAvailableDataOR>
			</channelGroupAvailableDataAND>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<threeComponentData/>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<ANDLocalSeismogramWrapper>
						<rMean/>
						<rMean/>
					</ANDLocalSeismogramWrapper>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<ORLocalSeismogramWrapper>
						<phaseSignalToNoise>
							<modelName>prem</modelName>
							<phaseName>ttp</phaseName>
							<shortOffsetBegin>
								<unit>SECOND</unit>
								<value>-5</value>
							</shortOffsetBegin>
							<shortOffsetEnd>
								<unit>SECOND</unit>
								<value>5</value>
							</shortOffsetEnd>
							<longOffsetBegin>
								<unit>SECOND</unit>
								<value>-105</value>
							</longOffsetBegin>
							<longOffsetEnd>
								<unit>SECOND</unit>
								<value>-5</value>
							</longOffsetEnd>
							<ratio>2</ratio>
						</phaseSignalToNoise>
					</ORLocalSeismogramWrapper>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<channelGroupLegacyExecute>
						<command>echo</command>
					</channelGroupLegacyExecute>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<channelGroupAND>
						<rMean/>
						<responseGain/>
					</channelGroupAND>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<channelGroupOR>
						<rMean/>
						<responseGain/>
					</channelGroupOR>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<channelGroupXOR>
						<threeComponentData/>
						<threeComponentData/>
					</channelGroupXOR>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<channelGroupFork>
				<channelGroupAlwaysSuccess>
					<channelGroupNOT>
						<responseGain/>
					</channelGroupNOT>
				</channelGroupAlwaysSuccess>
			</channelGroupFork>
			<alwaysSuccess>
				<phaseSignalToNoise>
					<modelName>prem</modelName>
					<phaseName>ttp</phaseName>
					<shortOffsetBegin>
						<unit>SECOND</unit>
						<value>-5</value>
					</shortOffsetBegin>
					<shortOffsetEnd>
						<unit>SECOND</unit>
						<value>5</value>
					</shortOffsetEnd>
					<longOffsetBegin>
						<unit>SECOND</unit>
						<value>-105</value>
					</longOffsetBegin>
					<longOffsetEnd>
						<unit>SECOND</unit>
						<value>-5</value>
					</longOffsetEnd>
					<ratio>0</ratio>
				</phaseSignalToNoise>
			</alwaysSuccess>
			
			<saveSeismogramToFile>
				<fileType>sac</fileType>
				<dataDirectory>BUD_II</dataDirectory>
				<eventDirLabel>
                    Event_<originTime>yyyy_DDD_HH_mm_ss</originTime>
				</eventDirLabel>
			</saveSeismogramToFile>
			<localSeismogramTemplateGenerator>
				<fileDir>status</fileDir>
				<seismogramConfig xlink:href="jar:edu/sc/seis/sod/data/templates/waveformArm/localSeismogram.xml"/>
			</localSeismogramTemplateGenerator>
		</motionVectorArm>
	</waveformArm>
</sod>
