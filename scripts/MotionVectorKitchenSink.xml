<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <properties>
        <runName>MotionVector KitchenSink</runName>
        <statusBase>status</statusBase>
        <eventQueryIncrement>
            <value>1</value>
            <unit>WEEK</unit>
        </eventQueryIncrement>
        <eventRefreshInterval>
            <value>6</value>
            <unit>MINUTE</unit>
        </eventRefreshInterval>
        <eventLag>
            <value>1</value>
            <unit>WEEK</unit>
        </eventLag>
        <maxRetryDelay>
            <value>16</value>
            <unit>DAY</unit>
        </maxRetryDelay>
        <waveformWorkerThreads>1</waveformWorkerThreads>
        <removeDatabase>TRUE</removeDatabase>
    </properties>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <boxArea>
                <latitudeRange>
                    <min>-90</min>
                    <max>90</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-180</min>
                    <max>180</max>
                </longitudeRange>
            </boxArea>
            <originDepthRange>
                <unit>KILOMETER</unit>
                <min>0</min>
                <max>1000</max>
            </originDepthRange>
            <originTimeRange>
                <startTime>20030101T00:00:00.001Z</startTime>
                <endTime>20030131T23:59:59.999Z</endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>6.0</min>
                <max>10.0</max>
            </magnitudeRange>
            <catalog>PREF</catalog>
            <contributor>IRIS</contributor>
        </eventFinder>
        <removeEventDuplicate/>
        <printLineEventProcess/>
        <eventStatusTemplate>
            <template>jar:edu/sc/seis/sod/data/templates/eventArm/eventStatus.xml</template>
        </eventStatusTemplate>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
            <refreshInterval>
                <unit>HOUR</unit>
                <value>50</value>
            </refreshInterval>
        </networkFinder>
        <networkCode>II</networkCode>
        <stationEffectiveTimeOverlap>
            <startTime>20030101T00:00:00.001Z</startTime>
            <endTime>20030131T23:59:59.999Z</endTime>
        </stationEffectiveTimeOverlap>
        <siteAND>
            <siteOR>
                <siteCode/>
                <siteCode>00</siteCode>
            </siteOR>
            <siteEffectiveTimeOverlap>
                <startTime>20030101T00:00:00.001Z</startTime>
                <endTime>20030131T23:59:59.999Z</endTime>
            </siteEffectiveTimeOverlap>
        </siteAND>
        <channelAND>
            <bandCode>L</bandCode>
            <channelNOT>
                <!-- to prevent LOG channels -->
                <gainCode>O</gainCode>
            </channelNOT>
            <channelEffectiveTimeOverlap>
                <startTime>20030101T00:00:00.001Z</startTime>
                <endTime>20030131T23:59:59.999Z</endTime>
            </channelEffectiveTimeOverlap>
        </channelAND>
        <networkInfoTemplateGenerator>
            <netConfig>
                <template>jar:edu/sc/seis/sod/data/templates/networkArm/networks.xml</template>
            </netConfig>
            <stationConfig>
                <template>jar:edu/sc/seis/sod/data/templates/networkArm/stations.xml</template>
            </stationConfig>
            <channelConfig>
                <template>jar:edu/sc/seis/sod/data/templates/networkArm/channels.xml</template>
            </channelConfig>
        </networkInfoTemplateGenerator>
    </networkArm>
    <waveformVectorArm>
        <eventVectorAND>
            <eventVectorOR>
                <ANDEventChannelWrapper>
                    <embeddedChannel>
                        <bandCode>B</bandCode>
                    </embeddedChannel>
                </ANDEventChannelWrapper>
                <passEventChannel/>
            </eventVectorOR>
            <eventVectorOR>
                <OREventChannelWrapper>
                    <embeddedChannel>
                        <bandCode>B</bandCode>
                    </embeddedChannel>
                </OREventChannelWrapper>
                <passEventChannel/>
            </eventVectorOR>
            <eventVectorOR>
                <eventVectorNOT>
                    <OREventChannelWrapper>
                        <embeddedChannel>
                            <bandCode>B</bandCode>
                        </embeddedChannel>
                    </OREventChannelWrapper>
                </eventVectorNOT>
                <passEventChannel/>
            </eventVectorOR>
        </eventVectorAND>
        <requestGeneratorWrapper>
            <phaseRequest>
                <model>prem</model>
                <beginPhase>ttp</beginPhase>
                <beginOffset>
                    <unit>SECOND</unit>
                    <value>-120</value>
                </beginOffset>
                <endPhase>2.0kmps</endPhase>
                <endOffset>
                    <unit>SECOND</unit>
                    <value>60</value>
                </endOffset>
            </phaseRequest>
        </requestGeneratorWrapper>
        <vectorRequestAND>
            <vectorRequestOR>
                <ANDRequestWrapper>
                    <breqFastRequest>
                        <dataDirectory>request</dataDirectory>
                        <name>Bob the Seismologist</name>
                        <inst>USC</inst>
                        <mail>200 Main Street, GreatCity,USA</mail>
                        <email>bob@seis.sc.edu</email>
                        <phone>8033332223</phone>
                        <fax>none</fax>
                        <media>Electronic</media>
                        <altmedia1>Electronic</altmedia1>
                        <altmedia2>Electronic</altmedia2>
                        <quality>A</quality>
                    </breqFastRequest>
                </ANDRequestWrapper>
                <passVectorRequest/>
            </vectorRequestOR>
            <vectorRequestOR>
                <ORRequestWrapper>
                    <breqFastRequest>
                        <dataDirectory>request</dataDirectory>
                        <name>Bob the Seismologist</name>
                        <inst>USC</inst>
                        <mail>200 Main Street, GreatCity,USA</mail>
                        <email>bob@seis.sc.edu</email>
                        <phone>8033332223</phone>
                        <fax>none</fax>
                        <media>Electronic</media>
                        <altmedia1>Electronic</altmedia1>
                        <altmedia2>Electronic</altmedia2>
                        <quality>A</quality>
                    </breqFastRequest>
                </ORRequestWrapper>
                <passVectorRequest/>
            </vectorRequestOR>
            <vectorRequestOR>
                <vectorRequestNOT>
                    <ANDRequestWrapper>
                        <requestPrint/>
                    </ANDRequestWrapper>
                </vectorRequestNOT>
                <passVectorRequest/>
            </vectorRequestOR>
        </vectorRequestAND>
        <fixedDataCenter>
            <name>IRIS_BudDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <vectorAvailableDataAND>
            <vectorAvailableDataOR>
                <ANDAvailableDataWrapper>
                    <noGaps/>
                </ANDAvailableDataWrapper>
                <passVectorAvailableData/>
            </vectorAvailableDataOR>
            <vectorAvailableDataOR>
                <ORAvailableDataWrapper>
                    <someCoverage/>
                </ORAvailableDataWrapper>
                <passVectorAvailableData/>
            </vectorAvailableDataOR>
            <vectorAvailableDataOR>
                <vectorAvailableDataNOT>
                    <ANDAvailableDataWrapper>
                        <noGaps/>
                    </ANDAvailableDataWrapper>
                </vectorAvailableDataNOT>
                <passVectorAvailableData/>
            </vectorAvailableDataOR>
        </vectorAvailableDataAND>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <threeComponentData/>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <ANDWaveformProcessWrapper>
                    <rMean/>
                    <rMean/>
                </ANDWaveformProcessWrapper>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <ORWaveformProcessWrapper>
                    <phaseSignalToNoise>
                        <modelName>prem</modelName>
                        <phaseName>ttp</phaseName>
                        <shortOffsetBegin>
                            <unit>SECOND</unit>
                            <value>-5</value>
                        </shortOffsetBegin>
                        <shortOffsetEnd>
                            <unit>SECOND</unit>
                            <value>5</value>
                        </shortOffsetEnd>
                        <longOffsetBegin>
                            <unit>SECOND</unit>
                            <value>-105</value>
                        </longOffsetBegin>
                        <longOffsetEnd>
                            <unit>SECOND</unit>
                            <value>-5</value>
                        </longOffsetEnd>
                        <ratio>2</ratio>
                    </phaseSignalToNoise>
                </ORWaveformProcessWrapper>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <legacyVectorExecute>
                    <command>echo</command>
                </legacyVectorExecute>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <waveformVectorAND>
                    <rMean/>
                    <responseGain/>
                </waveformVectorAND>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <waveformVectorOR>
                    <rMean/>
                    <responseGain/>
                </waveformVectorOR>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <waveformVectorFork>
            <waveformVectorAlwaysSuccess>
                <waveformVectorNOT>
                    <responseGain/>
                </waveformVectorNOT>
            </waveformVectorAlwaysSuccess>
        </waveformVectorFork>
        <alwaysSuccess>
            <phaseSignalToNoise>
                <modelName>prem</modelName>
                <phaseName>ttp</phaseName>
                <shortOffsetBegin>
                    <unit>SECOND</unit>
                    <value>-5</value>
                </shortOffsetBegin>
                <shortOffsetEnd>
                    <unit>SECOND</unit>
                    <value>5</value>
                </shortOffsetEnd>
                <longOffsetBegin>
                    <unit>SECOND</unit>
                    <value>-105</value>
                </longOffsetBegin>
                <longOffsetEnd>
                    <unit>SECOND</unit>
                    <value>-5</value>
                </longOffsetEnd>
                <ratio>0</ratio>
            </phaseSignalToNoise>
        </alwaysSuccess>
        <saveSeismogramToFile>
            <fileType>sac</fileType>
            <dataDirectory>BUD_II</dataDirectory>
        </saveSeismogramToFile>
        <localSeismogramTemplateGenerator>
            <template>jar:edu/sc/seis/sod/data/templates/waveformArm/localSeismogram.xml</template>
        </localSeismogramTemplateGenerator>
    </waveformVectorArm>
</sod>
