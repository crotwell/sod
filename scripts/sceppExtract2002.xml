<?xml version="1.0" encoding="UTF-8"?>
<sod xmlns:xlink="http://www.w3.org/1999/xlink/">
    <properties>
        <runName>SCEPP Extract 2002</runName>
        <statusBase>status</statusBase>
        <eventQueryIncrement>
            <value>1</value>
            <unit>WEEK</unit>
        </eventQueryIncrement>
        <eventRefreshInterval>
            <value>6</value>
            <unit>MINUTE</unit>
        </eventRefreshInterval>
        <eventLag>
            <value>1</value>
            <unit>WEEK</unit>
        </eventLag>
        <maxRetryDelay>
            <value>1</value>
            <unit>NANOSECOND</unit>
        </maxRetryDelay>
        <waveformWorkerThreads>1</waveformWorkerThreads>
        <eventChannelPairProcessing>atLeastOnce</eventChannelPairProcessing>
    </properties>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <boxArea>
                <latitudeRange>
                    <unit>DEGREE</unit>
                    <min>-90</min>
                    <max>90</max>
                </latitudeRange>
                <longitudeRange>
                    <unit>DEGREE</unit>
                    <min>-180</min>
                    <max>180</max>
                </longitudeRange>
            </boxArea>
            <originDepthRange>
                    <unit>KILOMETER</unit>
                    <min>0</min>
                    <max>1000</max>
            </originDepthRange>
            <originTimeRange>
                <startTime>20020101T00:00:00.001Z</startTime>
                <endTime>20021231T23:59:59.999Z</endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>3</min>
                <max>10.0</max>
            </magnitudeRange>
            <catalog>PREF</catalog>
        </eventFinder>
        <originAND>
            <removeEventDuplicate/>
            <originOR>
            <originAND>
                    <magnitudeRange>
                        <min>5.1</min>
                        <max>10.0</max>
                    </magnitudeRange>
                    <originDepthRange>
                            <unit>KILOMETER</unit>
                            <min>200</min>
                            <max>1000</max>
                    </originDepthRange>
            </originAND>

            <originAND>
                    <magnitudeRange>
                        <min>5.3</min>
                        <max>10.0</max>
                    </magnitudeRange>
                    <originDepthRange>
                            <unit>KILOMETER</unit>
                            <min>100</min>
                            <max>200</max>
                    </originDepthRange>
            </originAND>

            <originAND>
                <originPointDistance>
                    <latitude>34</latitude>
                    <longitude>-81</longitude>
                    <unit>DEGREE</unit>
                    <min>0</min>
                    <max>10</max>
                </originPointDistance>
                    <magnitudeRange>
                        <min>3.0</min>
                        <max>10.0</max>
                    </magnitudeRange>
            </originAND>

            <originAND>
                <originPointDistance>
                    <latitude>34</latitude>
                    <longitude>-81</longitude>
                    <unit>DEGREE</unit>
                    <min>10</min>
                    <max>30</max>
                </originPointDistance>
                    <magnitudeRange>
                        <min>4.0</min>
                        <max>10.0</max>
                    </magnitudeRange>
            </originAND>

            <pointLinearDistanceMagnitude>
                    <latitude>34</latitude>
                    <longitude>-81</longitude>
                <unit>DEGREE</unit>
                <min>30</min>
                <max>60</max>
                <magnitudeRange>
                    <min>4.9</min>
                    <max>5.5</max>
                </magnitudeRange>
            </pointLinearDistanceMagnitude>

            <originAND>
                <originPointDistance>
                    <latitude>34</latitude>
                    <longitude>-81</longitude>
                    <unit>DEGREE</unit>
                    <min>60</min>
                    <max>180</max>
                </originPointDistance>
                    <magnitudeRange>
                        <min>5.5</min>
                        <max>10.0</max>
                    </magnitudeRange>
            </originAND>

        </originOR>
        </originAND>
        <printLineEventProcess/>
        <eventStatusTemplate>
            <eventConfig xlink:href="jar:edu/sc/seis/sod/data/templates/eventArm/eventStatus.xml"/>
        </eventStatusTemplate>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>SCEPPNetworkDC</name>
            <dns>edu/sc/seis</dns>
            <refreshInterval>
                <unit>HOUR</unit>
                <value>50</value>
            </refreshInterval>
        </networkFinder>
        <networkInfoTemplateGenerator>
            <netConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/networks.xml"/>
            <stationConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/stations.xml"/>
            <channelConfig xlink:href="jar:edu/sc/seis/sod/data/templates/networkArm/channels.xml"/>
        </networkInfoTemplateGenerator>
    </networkArm>
    <waveformArm>

        <localSeismogramArm>
            <phaseRequest>
                <model>prem</model>
                <beginPhase>ttp</beginPhase>
                <beginOffset>
                    <unit>SECOND</unit>
                    <value>-60</value>
                </beginOffset>
                <endPhase>tts</endPhase>
                <endOffset>
                    <unit>SECOND</unit>
                    <value>360</value>
                </endOffset>
            </phaseRequest>
            <fixedDataCenter>
                <name>SCEPPSeismogramDC</name>
                <dns>edu/sc/seis</dns>
            </fixedDataCenter>
            <someCoverage/>
            <printlineSeismogramProcess/>

            <saveSeismogramToFile>
                <fileType>sac</fileType>
                <dataDirectory>SCEPP</dataDirectory>
                <eventDirLabel>Event_<originTime>yyyy_DDD_HH_mm_ss</originTime>
                </eventDirLabel>
            </saveSeismogramToFile>
            <localSeismogramTemplateGenerator>
                <seismogramConfig xlink:href="jar:edu/sc/seis/sod/data/templates/waveformArm/localSeismogram.xml"/>
            </localSeismogramTemplateGenerator>
        </localSeismogramArm>
        <waveformEventTemplateGenerator>
            <waveformConfig xlink:href="jar:edu/sc/seis/sod/data/templates/waveformArm/waveformEvent.xml"/>
        </waveformEventTemplateGenerator>
        <waveformNetworkStatus>
            <template>jar:edu/sc/seis/sod/data/templates/waveformArm/waveformNetwork.xml</template>
            <networkListTemplate>jar:edu/sc/seis/sod/data/templates/waveformArm/waveformNetworks.xml</networkListTemplate>
        </waveformNetworkStatus>

        <waveformStationStatus>
            <template>jar:edu/sc/seis/sod/data/templates/waveformArm/waveformStation.xml</template>
        </waveformStationStatus>

    </waveformArm>
</sod>


