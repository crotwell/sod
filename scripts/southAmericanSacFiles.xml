<?xml version="1.0" encoding="UTF-8"?>
<sod>
    <eventArm>
        <eventFinder>
            <name>IRIS_EventDC</name>
            <dns>edu/iris/dmc</dns>
            <boxArea>
                <latitudeRange>
                    <min>-30</min>
                    <max>10</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-85</min>
                    <max>-55</max>
                </longitudeRange>
            </boxArea>
            <originDepthRange>
                <unit>KILOMETER</unit>
                <min>0</min>
                <max>350</max>
            </originDepthRange>
            <originTimeRange>
                <startTime>
                    <year>1995</year>
                    <month>1</month>
                    <day>1</day>
                </startTime>
                <endTime>
                    <now/>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>5.0</min>
            </magnitudeRange>
            <catalog>WHDF</catalog>
        </eventFinder>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <networkFinder>
            <name>IRIS_NetworkDC</name>
            <dns>edu/iris/dmc</dns>
        </networkFinder>
        <bandCode>B</bandCode>
        <gainCode>H</gainCode>
        <printlineChannelProcessor/>
    </networkArm>
    <waveformVectorArm>
        <distanceRange>
            <unit>DEGREE</unit>
            <min>30</min>
        </distanceRange>
        <OREventChannelWrapper>
            <channelCode>BHZ</channelCode>
        </OREventChannelWrapper>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>ttp</beginPhase>
            <beginOffset>
                <unit>MINUTE</unit>
                <value>0</value>
            </beginOffset>
            <endPhase>ttp</endPhase>
            <endOffset>
                <unit>SECOND</unit>
                <value>180</value>
            </endOffset>
        </phaseRequest>
        <fixedDataCenter>
            <name>IRIS_PondDataCenter</name>
            <dns>edu/iris/dmc</dns>
        </fixedDataCenter>
        <someCoverage/>
        <printlineSeismogramProcess/>
        <rMean/>
        <rTrend/>
        <sacWriter/>
    </waveformVectorArm>
</sod>
