<?xml version="1.0" encoding="UTF-8"?>
<?oxygen RNGSchema="file:/C:/cygwin/home/groves/sod/relax/sod.rng" type="xml"?>
<sod xmlns:xlink="http://www.w3.org/1999/xlink/">
        <properties>
                <property>
                        <name>edu.sc.seis.sod.databasetype</name>
                        <value>edu.sc.seis.sod.database.HSqlDbManager</value>
                </property>
                <!-- number of days to get events per request to eventFinder -->
                <property>
                        <name>edu.sc.seis.sod.daystoincrement</name>
                        <value>7</value>
                </property>
                <!-- don't go back and reask server for events that may have
         appeared in server after this was first run. -->
                <property>
                        <name>edu.sc.seis.sod.database.getNewEvents</name>
                        <value>false</value>
                </property>
        </properties>
        <eventArm>
                <eventFinder>
                         <name>ANHINGAEVENTDC</name>
                         <dns>edu/sc/seis</dns>

                        <boxArea>
                                <latitudeRange>
                                        <unit>DEGREE</unit>
                                        <min>-90</min>
                                        <max>90</max>
                                </latitudeRange>
                                <longitudeRange>
                                        <unit>DEGREE</unit>
                                        <min>-180</min>
                                        <max>180</max>
                                </longitudeRange>
                        </boxArea>
                        <originDepthRange>
                                <unitRange>
                                        <unit>KILOMETER</unit>
                                        <min>0</min>
                                        <max>1000</max>
                                </unitRange>
                        </originDepthRange>
                        <eventTimeRange>
                                <timeRange>
                                        <startTime>20030101T00:00:00.001Z</startTime>
                                        <endTime>20031131T23:59:59.999Z</endTime>
                                </timeRange>
                        </eventTimeRange>
                        <magnitudeRange>
                                <magType>M</magType>
                                <min>5.0</min>
                                <max>10.0</max>
                        </magnitudeRange>
                        <catalog>BIGQUAKE</catalog>
                        <contributor>NEIC</contributor>
                </eventFinder>
                <simpleHTMLEventStatus>
                        <outputDirectory>scepptutorialStatus/</outputDirectory>
                </simpleHTMLEventStatus>
                <mapEventStatus xlink:link="scepptutorialStatus/eventMap.png"/>
                <eventStatusTemplate
                       outputLocation="scepptutorialStatus/time.html"
                       xlink:link="jar:edu/sc/seis/sod/data/templates/eventArm/timeSorted.xml"/>
               <eventStatusTemplate
                       outputLocation="scepptutorialStatus/depth.html"
                       xlink:link="jar:edu/sc/seis/sod/data/templates/eventArm/depthSorted.xml"/>
               <eventStatusTemplate
                       outputLocation="scepptutorialStatus/location.html"
                       xlink:link="jar:edu/sc/seis/sod/data/templates/eventArm/locSorted.xml"/>
               <eventStatusTemplate
                       outputLocation="scepptutorialStatus/mag.html"
                       xlink:link="jar:edu/sc/seis/sod/data/templates/eventArm/magSorted.xml"/>

        </eventArm>
        <networkArm>
                <networkFinder>
                        <description>Connect to the IRIS networkDC</description>
                        <name>SCEPPNetworkDC</name>
                        <dns>edu/sc/seis</dns>
                        <refreshInterval>
                                <unit>MINUTE</unit>
                                <value>3000</value>
                        </refreshInterval>
                </networkFinder>
                <networkCode>SP</networkCode>
                <stationEffectiveTimeOverlap>
                        <effectiveTimeOverlap>
                                <min>20030101T00:00:00.001Z</min>
                                <max>20030131T23:59:59.999Z</max>
                        </effectiveTimeOverlap>
                </stationEffectiveTimeOverlap>
                <siteAND>
                        <siteOR>
                                <siteCode>  </siteCode>
                                <siteCode>00</siteCode>
                        </siteOR>
                        <siteEffectiveTimeOverlap>
                                <effectiveTimeOverlap>
                                        <min>20030101T00:00:00.001Z</min>
                                        <max>20030131T23:59:59.999Z</max>
                                </effectiveTimeOverlap>
                        </siteEffectiveTimeOverlap>
                </siteAND>
                <channelAND>
                        <bandCode>B</bandCode>
                        <channelNOT>
                                <!-- to prevent LOG channels -->
                                <gainCode>O</gainCode>
                        </channelNOT>
                        <channelEffectiveTimeOverlap>
                                <effectiveTimeOverlap>
                                        <min>20030101T00:00:00.001Z</min>
                                        <max>20030131T23:59:59.999Z</max>
                                </effectiveTimeOverlap>
                        </channelEffectiveTimeOverlap>
                </channelAND>
                <simpleHTMLNetworkStatus>
                        <outputDirectory>scepptutorialStatus/</outputDirectory>
                </simpleHTMLNetworkStatus>
        </networkArm>
        <waveFormArm>
                <distanceRange>
                        <unit>DEGREE</unit>
                        <min>30</min>
                        <max>90</max>
                </distanceRange>
                <localSeismogramArm>
                        <phaseRequest>
                                <model>prem</model>
                                <beginPhase>ttp</beginPhase>
                                <beginOffset>
                                        <unit>SECOND</unit>
                                        <value>-60</value>
                                </beginOffset>
                                <endPhase>tts</endPhase>
                                <endOffset>
                                        <unit>SECOND</unit>
                                        <value>360</value>
                                </endOffset>
                        </phaseRequest>
                        <fixedDataCenter>
                                <name>SCEPPSeismogramDC</name>
                                <dns>edu/sc/seis</dns>
                        </fixedDataCenter>
                        <someCoverage/>
                        <printlineSeismogramProcess/>
                        <sacFileProcessor>
                                <dataDirectory>POND_II</dataDirectory>
                                <eventDirLabel> Event_<originTime>yyyy_DDD_HH_mm_ss</originTime>
                                </eventDirLabel>
                        </sacFileProcessor>
                </localSeismogramArm>
                <simpleHTMLWaveformStatus>
                        <outputDirectory>scepptutorialStatus/</outputDirectory>
                </simpleHTMLWaveformStatus>
        </waveFormArm>
</sod>
