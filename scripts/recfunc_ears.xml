<?xml version="1.0"?>
<!-- generates receiver functions similar to EARS, http://ears.iris.washington.edu/
-->
<sod>
    <eventArm>
        <fdsnEvent>
            <originTimeRange>
                <startTime>
                    <year>2011</year>
                    <month>1</month>
                    <day>11</day>
                </startTime>
                <endTime>
                    <year>2011</year>
                    <month>1</month>
                    <day>13</day>
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>5</min>
            </magnitudeRange>
            <catalog>NEIC PDE</catalog>
            <contributor>NEIC PDE-M</contributor>
        </fdsnEvent>
        <originOR>
            <originAND>
                <magnitudeRange>
                    <min>6</min>
                </magnitudeRange>
                <originDepthRange>
                    <unit>KILOMETER</unit>
                    <min>100</min>
                </originDepthRange>
            </originAND>
        </originOR>
        <printlineEventProcess/>
    </eventArm>
    <networkArm>
        <fdsnStation/>
        <networkCode>IU</networkCode>
        <stationOR>
           <stationCode>YAK</stationCode>
           <stationCode>BILL</stationCode>
           <stationCode>KIEV</stationCode>
           <stationCode>ULN</stationCode>
        </stationOR>
        <bandCode>B</bandCode>
        <printlineChannelProcess/>
    </networkArm>
    <waveformVectorArm>
        <distanceRange>
            <unit>DEGREE</unit>
            <min>30</min>
            <max>90</max>
        </distanceRange>
        <phaseRequest>
            <model>prem</model>
            <beginPhase>P</beginPhase>
            <beginOffset>
                <unit>SECOND</unit>
                <value>-120</value>
            </beginOffset>
            <endPhase>tts</endPhase>
            <endOffset>
                <unit>SECOND</unit>
                <value>180</value>
            </endOffset>
        </phaseRequest>
        <fdsnDataSelect/>
        <someCoverage/>
        <fullDataCoverage/>
        <vectorTrim/>

        <phaseCut>
            <beginPhase>P</beginPhase>
            <beginOffset>
                <unit>SECOND</unit>
                <value>-30</value>
            </beginOffset>
            <endPhase>P</endPhase>
            <endOffset>
                <unit>SECOND</unit>
                <value>120</value>
            </endOffset>
        </phaseCut>
        <threeComponentData/>
        <rMean/>
        <rTrend/>
        <taper/>
        <responseGain/>
        <filter>
            <lowFreqCorner>
                <value>50</value>
                <unit>SECOND</unit>
            </lowFreqCorner>
            <highFreqCorner>
                <value>5</value>
                <unit>HERTZ</unit>
            </highFreqCorner>
            <numPoles>2</numPoles>
            <filterType>NONCAUSAL</filterType>
        </filter>
        <sacWriter/>
        <iterDeconReceiverFunction>
            <sacWriter/>
        </iterDeconReceiverFunction>
        <printlineSeismogramProcess/>
    </waveformVectorArm>
</sod>
