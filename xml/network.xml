	</networkeffectiveTimeOverlap>

	<stationAND>
		  <stationArea>
		    <boxArea>
			<latitudeRange>
				<min>30</min>
				<max>33</max>
			</latitudeRange>
			<longitudeRange>
				<min>-100</min>
				<max>100</max>
			</longitudeRange>
		    </boxArea>
		   </stationArea>
		<stationeffectiveTimeOverlap>
			<effectiveTimeOverlap>
				<min>1999-01-01T00:00:00Z</min>
				<max>2000-01-01T00:00:00Z</max>
			</effectiveTimeOverlap>
		</stationeffectiveTimeOverlap>
	</stationAND>

	<siteCode><value>00</value></siteCode>

	<channelIDOR>
		<broadband/>
		<longPeriod/>
	</channelIDOR>

	<sampling>
		<min>1</min>
		<max>40</max>
		<interval>
			<unit>SECOND</unit>
			<value>1</value>
		</interval>
	</sampling>

	<printlineChannelProcessor/>
	
    </networkArm>
</sod>
