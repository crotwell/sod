<?xml version="1.0" encoding="UTF-8"?>
<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"
        xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <start>
                <choice>
                        <element name="simpleHTMLWaveformStatus">
                                <optional>
                                        <element name="outputDirectory">
                                                <data type="string"/>
                                        </element>
                                </optional>
                                <optional>
                                        <element name="eventDirLabel">
                                                <ref name="eventLabel"/>
                                        </element>
                                </optional>
                        </element>
                        <element name="waveformEventTemplateGenerator">
                                <optional>
                                    <element name="fileDir">
                                        <data type="string"/>
                                    </element>
                                </optional>
                                <element name="waveformConfig">
                                        <attribute name="href" ns="http://www.w3.org/1999/xlink/"/>
                                </element>
                        </element>
                        <element name="waveformNetworkStatus">
                                <optional>
                                    <element name="fileDir">
                                        <data type="string"/>
                                    </element>
                                </optional>
                                <element name="template">
                                        <data type="string"/>
                                </element>
                                <element name="networkListTemplate">
                                        <data type="string"/>
                                </element>
                        </element>
                        <element name="waveformStationStatus">
                                <optional>
                                    <element name="fileDir">
                                        <data type="string"/>
                                    </element>
                                </optional>
                                <element name="template">
                                        <data type="string"/>
                                </element>
                        </element>
                </choice>
        </start>
        <include href="../base/include/eventLabel.rng"/>
</grammar>

