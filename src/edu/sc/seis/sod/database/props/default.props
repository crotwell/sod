eventstatus.create=CREATE TABLE eventstatus( eventid int, eventcondition int, \
                                             PRIMARY KEY( eventid ))
querytimes.create=CREATE TABLE querytimes (query_time timestamp, \
                                           server varchar, \
                                           dns varchar, \
                                           UNIQUE (server, dns))
eventchannelstatus.create=CREATE TABLE eventchannelstatus( pairid int, \
                                                           eventid int, \
                                                           channelid int, \
                                                           status int, \
                                                           PRIMARY KEY (pairid), \
                                                           UNIQUE (eventid, channelid))
eventchannelstatus.index_evchanstatus=CREATE INDEX evchanstatus_chan ON eventchannelstatus( channelid)

eventchannelretry.create=CREATE TABLE eventchannelretry( pairid int, \
                                                         numfailure int DEFAULT 0, \
                                                         retry_time timestamp, \
                                                         PRIMARY KEY (pairid))
eventchannelcookiejar.create=CREATE TABLE eventchannelcookiejar( pairid int, \
                                                                 name varchar, \
                                                                 value double, \
                                                                 valuestring varchar, \
                                                                 valueobject object, \
                                                                 UNIQUE (pairid, name))
RecordSectionSeq.create=CREATE SEQUENCE RecordSectionSeq
RecordSectionSeq.nextVal=SELECT NEXTVAL ('RecordSectionSeq')

EventChannelSeq.create=CREATE SEQUENCE EventChannelSeq
EventChannelSeq.nextVal=SELECT NEXTVAL ('EventChannelSeq')

version.create=CREATE TABLE version( dbversion varchar, \
                                     PRIMARY KEY (dbversion) )

config.create=CREATE TABLE config( configString varchar, \
                                   PRIMARY KEY (configString) )

networkstatus.create=CREATE TABLE networkstatus( networkid int, status int, \
                                                 FOREIGN KEY(networkid) \
                                                 REFERENCES network (net_id))

newchannels.create=CREATE TABLE newchannels( channelid int, \
                                             FOREIGN KEY(channelid) \
                                             REFERENCES channel (chan_id))
                                             
recsecchannel.create=CREATE TABLE recsecchannel \
                 (recSecId varchar, eq_dbid int, channelid int,topLeftX double, topLeftY double,bottomRightX double, \
                bottomRightY double, best int, internalId int)

recsecchannel.index_rec_eq=CREATE INDEX rec_eq_index ON recsecchannel (eq_dbid)

recsecchannel.index_rec_best_eq=CREATE INDEX rec_best_eq ON recsecchannel (recsecid, eq_dbid, best)

status.create=CREATE TABLE status ( id int, name varchar )
