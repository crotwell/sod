<?xml version="1.0"?>
<html>
<header><title>Networks in the waveform arm</title></header>
<body>
<table>
#foreach ( $net in $successful_networks )
#set ($netyear = $fissures.networkCodeYear($net.getNetworkAccess()) )
#set ($netCode = $fissures.formatNetwork($net.getNetworkAccess().get_attributes().get_id()) )
<tr>
#if ($net.getNetworkAccess().get_attributes().get_code().startsWith("X") || $net.getNetworkAccess().get_attributes().get_code().startsWith("X") || $net.getNetworkAccess().get_attributes().get_code().startsWith("X"))
<td>$netyear</td>
#else
<td>$net.getNetworkAccess().get_attributes().get_code()</td>
#end
<td><a href="waveformStations/${netCode}.html">$fissures.networkName($net.getNetworkAccess())</a></td>
</tr>
#end
</table>
</body>
</html></template>


