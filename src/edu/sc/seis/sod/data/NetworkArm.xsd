			<xsd:extension base="subsetterType"/>
		</xsd:complexContent>
	</xsd:complexType>

	<!-- NETWORKFINDER TYPE-->
	<xsd:complexType name="networkFinderType">
		<xsd:complexContent>
			<xsd:extension base="networkSource">
				<xsd:sequence>	
					<xsd:element ref="refreshInterval" minOccurs = "0" maxOccurs="1"/>
				</xsd:sequence>
