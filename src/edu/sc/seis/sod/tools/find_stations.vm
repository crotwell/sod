#if($additionalArms.length() > 0)
$additionalArms#end
    <networkArm>
        <networkFinder>
            <name>$server.name</name>
            <dns>$server.dns</dns>
        </networkFinder>
#if($networks)
        <networkOR>
#foreach($code in $networks)
            <networkCode>$code</networkCode>
#end
        </networkOR>
#end
#if($needsStationAND)
        <stationAND>
#if($stations)
            <stationOR>
#foreach($code in $stations)
                <stationCode>$code</stationCode>
#end
            </stationOR>
#end
#if($box)
            <stationArea>
                <boxArea>
                    <latitudeRange>
                        <min>$box.south</min>
                        <max>$box.north</max>
                    </latitudeRange>
                    <longitudeRange>
                        <min>$box.west</min>
                        <max>$box.east</max>
                    </longitudeRange>
                </boxArea>
            </stationArea>
#end
#if($donut)
            <stationPointDistance>
                <unit>DEGREE</unit>
                <min>$donut.min</min>
                <max>$donut.max</max>
                <latitude>$donut.lat</latitude>
                <longitude>$donut.lon</longitude>
            </stationPointDistance>
#end
#if($output)
            <printlineStationProcess>
                <template>$output</template>
            </printlineStationProcess>
#end
        </stationAND>
#end
    </networkArm>
</sod>