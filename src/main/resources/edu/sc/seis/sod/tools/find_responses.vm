    <networkArm>
        <fdsnStation>
#if($baseurl)
            <baseurl>$baseurl</baseurl>
#end
#foreach($code in $networks)
            <networkCode>$code</networkCode>
#end
#foreach($code in $stations)
            <stationCode>$code</stationCode>
#end
#foreach($code in $sites)
            <locationCode>$code</siteLocation>
#end
#foreach($code in $channels)
            <channelCode>$code</channelCode>
#end
        </fdsnStation>
#if($networks)
        <networkOR>
#foreach($code in $networks)
            <networkCode>$code</networkCode>
#end
        </networkOR>
#end
        <stationAND>
#if($stations)
            <stationOR>
#foreach($code in $stations)
                <stationCode>$code</stationCode>
#end
            </stationOR>
#end
#if($needsEffectiveTimeRange)
            <stationEffectiveTimeOverlap>
                <startTime>
                    $begin
                </startTime>
                <endTime>
                    $end
                </endTime>
            </stationEffectiveTimeOverlap>
#end
#if($box)
            <stationArea>
                <boxArea>
                    <latitudeRange>
                        <min>$box.south</min>
                        <max>$box.north</max>
                    </latitudeRange>
                    <longitudeRange>
                        <min>$box.west</min>
                        <max>$box.east</max>
                    </longitudeRange>
                </boxArea>
            </stationArea>
#end
#if($donut)
            <stationPointDistance>
                <unit>DEGREES</unit>
                <min>$donut.min</min>
                <max>$donut.max</max>
                <latitude>$donut.lat</latitude>
                <longitude>$donut.lon</longitude>
            </stationPointDistance>
#end
        </stationAND>
#if($sites)
        <channelOR>
#foreach($code in $sites)
            <siteCode>$code</siteCode>
#end
		</channelOR>
#end
#if($needsEffectiveTimeRange)
        <channelEffectiveTimeOverlap>
            <startTime>
               $begin
            </startTime>
            <endTime>
               $end
            </endTime>
        </channelEffectiveTimeOverlap>
#end
#if($channels)
        <channelOR>
#foreach($code in $channels)
            <channelCode>$code</channelCode>
#end
        </channelOR>
#end
        <printlineChannelProcess/>
#if($type == 'polezero')
        <sacPoleZeroWriter>
            <workingDir></workingDir>
            <location>$fileTemplate</location>
        </sacPoleZeroWriter>
#else
        <responseWriter>
            <workingDir></workingDir>
            <location>$fileTemplate</location>
        </responseWriter>
#end
    </networkArm>
</sod>
