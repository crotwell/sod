<ehcache>

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
        maxElementsInMemory="1000"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="true"
        diskPersistent="false"
        diskExpiryThreadIntervalSeconds="120"
        memoryStoreEvictionPolicy="LRU"
        />
        
    <cache name="edu.iris.Fissures.model.UnitImpl"
        maxElementsInMemory="10"
        eternal="true"
        overflowToDisk="false"
        />
        
    <cache name="edu.iris.Fissures.network.NetworkAttrImpl"
        maxElementsInMemory="300"
        eternal="true"
        overflowToDisk="false"
        />
        
    <cache name="edu.iris.Fissures.network.StationImpl"
        maxElementsInMemory="3000"
        eternal="true"
        overflowToDisk="false"
        />
        
    <cache name="edu.iris.Fissures.network.ChannelImpl"
        maxElementsInMemory="12000"
        eternal="true"
        overflowToDisk="false"
        />
        
    <cache name="edu.sc.seis.fissuresUtil.cache.CacheEvent"
        maxElementsInMemory="100"
        eternal="true"
        overflowToDisk="false"
        />
        
    <cache name="edu.iris.Fissures.event.OriginImpl"
        maxElementsInMemory="100"
        eternal="true"
        overflowToDisk="false"
        />

</ehcache>
